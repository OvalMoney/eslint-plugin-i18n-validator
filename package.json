  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
